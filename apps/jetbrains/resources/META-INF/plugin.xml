<idea-plugin>
  <id>mac-native</id>
  <name>Mac Native Theme</name>
  <version>1.0</version>
  <vendor url="https://subtheme.pro/">Subtheme</vendor>

  <change-notes><![CDATA[
    <h3>1.0</h3>
    <ul>
      <li>Minor update to plugin.xml</li>
    </ul>
  ]]>
  </change-notes>
  <description><![CDATA[
    A native design for macOS with a dark and light theme.
    <br/><br/>
    Contribute with GitHub: <a href="https://github.com/subtheme-pro/mac-native">https://github.com/subtheme-pro/mac-native</a>
    <br/><br/>
    <img src="https://raw.githubusercontent.com/subtheme-pro/mac-native/master/apps/jetbrains/screenshots/mac-native-light-small.png" border="0" width="360">
    <br/>
    <img src="https://raw.githubusercontent.com/subtheme-pro/mac-native/master/apps/jetbrains/screenshots/mac-native-dark-small.png" border="0" width="360">
  ]]>
  </description>

  <idea-version since-build="191.0"/>

  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <themeProvider id="81674041-88e7-4df3-9507-245f17d087c0" path="/mac-native-light.theme.json"/>
    <themeProvider id="458f84b0-f6e6-414e-97ce-e166526075f1" path="/mac-native-dark.theme.json"/>
  </extensions>
</idea-plugin>
